{
    "profiles": [
        {
            "name": "coverage",
            "tty": true,
            "interactive": true,
            "arg_patterns": [
                {
                    "subcommand": "run"
                }
            ],
            "patterns": [
                {
                    "activation_expression": "^={70}$",
                    "deactivation_expression": "^-{70}$",
                    "deactivation_expression_line_offset": 1,
                    "expression": "^()(?:={70}|-{70}|(?:ERROR|FAIL): .*)()$",
                    "replace_groups": ["%C(bol;r)", "%Cz"]
                },
                {
                    "expression": "^Ran (\\d+) tests in ([0-9.]+s)$",
                    "replace_groups": ["%Hg", "%Hb"]
                },
                {
                    "expression": "OK(?: \\(skipped=(?P<skip>\\d+)\\))",
                    "replace_groups": {
                        "skip": "%Hy"
                    }
                },
                {
                    "expression": [
                        "FAILED \\(",
                            "(?:failures=(?P<fail>\\d+)(?:, )?)?",
                            "(?:errors=(?P<err>\\d+)(?:, )?)?",
                            "(?:skipped=(?P<skip>\\d+))?",
                        "\\)$"
                    ],
                    "replace_groups": {
                        "fail,err": "%Hlr",
                        "skip": "%Hy"
                    }
                }
            ]
        }
    ]
}

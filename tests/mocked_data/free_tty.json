{
	"profiles": [
		{
			"name": "free",
			"tty": true,
			"arg_patterns": [
				{
					"expression": "--help",
					"match_not": true
				}
			],
			"patterns": [
				{
					"expression": "^ +total",
					"skip_others": true
				},
				{
					"separator": " +",
					"field": 0,
					"min_fields": 7,
					"max_fields": 7,
					"expression": ".*",
					"replace_fields": {
						"2,5": "%Cb%Fc%Cz",
						"4,7": "%Cg%Fc%Cz"
					},
					"activation_line": 2
				},
				{
					"separator": " +",
					"field": 0,
					"min_fields": 4,
					"max_fields": 4,
					"expression": ".*",
					"replace_fields": {
						"2*4": "%Cy%Fc%Cz",
						"-4": "%C(lm)%Fc%Cz"
					},
					"activation_line": 2
				}
			]
		}
	]
}

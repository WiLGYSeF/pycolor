{
	"profiles": [
		{
			"name": "free",
			"arg_patterns": [
				{
					"expression": "--help",
					"match_not": true
				}
			],
			"patterns": [
				{
					"expression": "^ +total",
					"skip_others": true
				},
				{
					"expression": "^Mem:\\s+([0-9.]+[A-Z])\\s+([0-9.]+[A-Z])\\s+([0-9.]+[A-Z])",
					"replace_groups": {
						"*": "%Cg%Gc%Cz"
					},
					"activation_line": 2
				}
			]
		}
	]
}

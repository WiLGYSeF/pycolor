{
    "profiles": [
        {
            "name": "free",
            "arg_patterns": [
                {
                    "expression": "--help",
                    "match_not": true
                }
            ],
            "patterns": [
                {
                    "expression": "^ +total",
                    "skip_others": true
                },
                {
                    "separator": " +",
                    "field": "2*5",
                    "min_fields": 7,
                    "max_fields": 7,
                    "expression": ".*",
                    "replace": "%Cg%Fc%Cz",
                    "activation_line": 2
                },
                {
                    "separator": " +",
                    "field": "2*4*2,1",
                    "min_fields": 4,
                    "max_fields": 4,
                    "expression": ".*",
                    "replace": "%Cr%Fc%Cz",
                    "activation_line": 2
                }
            ]
        }
    ]
}

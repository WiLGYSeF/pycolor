{
    "profiles": [
        {
            "name": "free",
            "arg_patterns": [
                {
                    "expression": "--help",
                    "match_not": true
                }
            ],
            "patterns": [
                {
                    "expression": "^ +total",
                    "skip_others": true
                },
                {
                    "separator": " +",
                    "min_fields": 7,
                    "max_fields": 7,
                    "expression": "^[02468][0-9]*$",
                    "replace_all": "%Cy%Fc%Cz",
                    "activation_line": 2
                }
            ]
        }
    ]
}

{
    "profiles": [
        {
            "name": "ifconfig",
            "arg_patterns": [
                {
                    "expression": "-[A-Za-z]*s[A-Za-z]*",
                    "match_not": true
                }
            ],
            "patterns": [
                {
                    "expression": "^([A-Za-z0-9-]+): flags=([0-9]+)<([A-Z,]+)>  mtu ([0-9]+)",
                    "replace_groups": ["%C(bol;c)%Gc%Cz", "%Cg%Gc%Cz", "%Cm%Gc%Cz", "%Cg%Gc%Cz"]
                },
                {
                    "expression": "^ +inet ((?:[0-9]+\\.){3}[0-9]+)  netmask ((?:[0-9]+\\.){3}[0-9]+)  (?:broadcast|destination) ((?:[0-9]+\\.){3}[0-9]+)",
                    "replace_groups": ["%Cy%Gc%Cz", "%C(dim;y)%Gc%Cz", "%C(dim;y)%Gc%Cz"]
                },
                {
                    "expression": "^ +inet ((?:[0-9]+\\.){3}[0-9]+)  netmask ((?:[0-9]+\\.){3}[0-9]+)",
                    "replace_groups": ["%Cly%Gc%Cz", "%C(dim;y)%Gc%Cz"]
                },
                {
                    "expression": "^ +inet6 ([0-9a-f:]+)  prefixlen ([0-9]+)  scopeid (0x[0-9a-f]{2})<([a-z]+)>",
                    "replace_groups": ["%Cly%Gc%Cz", "%Cg%Gc%Cz", "%Cg%Gc%Cz", "%Cb%Gc%Cz"]
                },
                {
                    "expression": "^ +[a-z]+(?: ([0-9a-f:-]+))?  txqueuelen ([0-9]+)  \\(([A-Za-z0-9 ]+)\\)",
                    "replace_groups": ["%Cy%Gc%Cz", "%Cg%Gc%Cz", "%Cb%Gc%Cz"]
                },
                {
                    "super_expression": "^ +RX",
                    "expression": "[0-9]+",
                    "replace": "%Cc%G0%Cz"
                },
                {
                    "super_expression": "^ +RX",
                    "expression": "\\(([A-Z0-9. ]+)\\)",
                    "replace_groups": ["%Cc%G1%Cz"]
                },
                {
                    "super_expression": "^ +TX",
                    "expression": "[0-9]+",
                    "replace": "%Cg%G0%Cz"
                },
                {
                    "super_expression": "^ +TX",
                    "expression": "\\(([A-Z0-9. ]+)\\)",
                    "replace_groups": ["%Cg%G1%Cz"]
                }
            ]
        }
    ]
}

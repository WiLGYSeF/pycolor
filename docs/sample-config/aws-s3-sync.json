{
	"profiles": [
		{
			"name": "aws",
			"arg_patterns": [
				{
					"subcommand": ["s3", "sync"]
				}
			],
			"patterns": [
				{
					"expression": "^delete: (.*)$",
					"replace_groups": ["%C(bold;r)%Gc%Cz"]
				},
				{
					"expression": "^upload: (.*)$",
					"replace_groups": ["%C(bold;g)%Gc%Cz"]
				},
				{
					"expression": "^\\(dryrun\\) delete: (.*)$",
					"replace_groups": ["%Cr%Gc%Cz"]
				},
				{
					"expression": "^\\(dryrun\\) upload: (.*)$",
					"replace_groups": ["%Cg%Gc%Cz"]
				}
			]
		}
	]
}

{
	"profiles": [
		{
			"name": "df",
			"arg_patterns": [
				{
					"expression": "-[A-Za-z]*T[A-Za-z]*",
					"match_not": true
				},
				{
					"expression": "--output(=.*)?",
					"match_not": true
				},
				{
					"expression": "--(help|version)",
					"match_not": true
				}
			],
			"from_profiles": [
				{
					"name": "df_special",
					"order": "after"
				}
			],
			"patterns": [
				{
					"separator": " +",
					"field": 0,
					"expression": ".*",
					"replace_fields": {
						"2,5": "%Cb%Fc%Cz",
						"3": "%Cr%Fc%Cz",
						"4": "%Cg%Fc%Cz"
					},
					"activation_line": 2
				}
			]
		},
		{
			"name": "df",
			"arg_patterns": [
				{
					"expression": "-[A-Za-z]*T[A-Za-z]*"
				},
				{
					"expression": "--output(=.*)?",
					"match_not": true
				},
				{
					"expression": "--(help|version)",
					"match_not": true
				}
			],
			"from_profiles": [
				{
					"name": "df_special",
					"order": "after"
				}
			],
			"patterns": [
				{
					"separator": " +",
					"field": 0,
					"expression": ".*",
					"replace_fields": {
						"2": "%Ce%Fc%Cz",
						"3,6": "%Cb%Fc%Cz",
						"4": "%Cr%Fc%Cz",
						"5": "%Cg%Fc%Cz"
					},
					"activation_line": 2
				}
			]
		},
		{
			"profile_name": "df_special",
			"patterns": [
				{
					"separator": " +",
					"field": 1,
					"expression": "^[^/]+$",
					"replace": "%Ce%G0%Cz",
					"activation_line": 2
				}
			]
		}
	]
}

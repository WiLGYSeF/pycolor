{
	"profiles": [
		{
			"name": "iwconfig",
			"max_args": 0,
			"patterns": [
				{
					"expression": "^([A-Za-z0-9-]+)",
					"replace": "%C(bol;c)%G1%Cz"
				},
				{
					"expression": "ESSID:\"([^\"]+)\"",
					"replace_groups": ["%Cly%Gc%Cz"],
					"skip_others": true
				},
				{
					"expression": "Access Point: ((?:[0-9A-F]{2}:?){6})",
					"replace_groups": ["%Cy%Gc%Cz"],
					"skip_others": true
				},
				{
					"expression": "Link Quality=([0-9]+)/([0-9]+)",
					"replace_groups": ["%Cg%Gc%Cz", "%Cg%Gc%Cz"]
				},
				{
					"expression": "(?:[a-z][:=] *)(-?[0-9.]+(?: [A-Za-z/]+)?)",
					"replace_groups": ["%Cg%Gc%Cz"]
				},
				{
					"expression": "(?:[a-z][:=] *)([A-Za-z]+)",
					"replace_groups": ["%Clg%Gc%Cz"]
				},
				{
					"expression": "no wireless extensions\\.",
					"replace": "%Ce%G0%Cz"
				}
			]
		}
	]
}
